    <Content Include="Icons.xml" />
    <Content Include="Leaders.xml" />
    <Content Include="Text.xml" />
    <Content Include="Text_ja_JP.xml" />
    <Content Include="UU.xml" />
    <Content Include="Leaders_Movement.sql" />
    <Content Include="Leaders_Boost.sql" />
    <Content Include="Leader_grant_technology.sql" />
    <Content Include="Leader_post_combat_yield.sql" />
    <Content Include="Leader_grant_unit.sql" />
    <Content Include="Leader_city_yield.sql" />
    <Content Include="Leader_unit_production.xml" />
    <Content Include="Leader_district_production.sql" />
    <Content Include="Leader_government_slot.sql" />
    <Content Include="Leader_great_person_points.xml" />
    <Content Include="Leader_city_growth.sql" />
    <Content Include="Leader_heal_per_turn.sql" />
    <Content Include="Leader_plot_yeld_feature.sql" />
    <Content Include="Leader_plot_yeld_terrain.sql" />
    <Content Include="Leader_combat.sql" />
    <Content Include="Leader_wars_production.sql" />
    <Content Include="Leader_trade_route.sql" />
    <Content Include="Leader_enable_unit_purchase_faith.sql" />
    <Content Include="Leader_resource_visibility.sql" />
    <Content Include="Leader_diplomatic.sql" />

    <Content Include="policy.sql" />
    <Content Include="policy_text.xml" />
    <Content Include="policy_text_ja_JP.xml" />
    <None Include="Textures\FALLBACK_NEUTRAL_JASPERKITTY.dds" />
    <None Include="Textures\FALLBACK_NEUTRAL_JASPERKITTY.tex" />
    <None Include="Textures\LEADER_JASPER_KITTY_NEUTRAL.dds" />
    <None Include="Textures\LEADER_JASPER_KITTY_NEUTRAL.tex" />
    <None Include="ArtDefs\FallbackLeaders.artdef" />
    <None Include="XLPs\LeaderFallbacks.xlp" />
    <None Include="XLPs\UILeaders.xlp" />
    <None Include="NewLeader1.Art.xml" />
    <None Include="GettingStarted.html" />
  </ItemGroup>
  <Import Project="$(MSBuildLocalExtensionPath)Civ6.targets" />
</Project>